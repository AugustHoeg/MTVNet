
{
  "task": "superresolution",
  "model": "plain",
  "model_architecture": "mDCSRN",

  // WandB parameters
  "wandb_entity": "aulho",
  "wandb_project": "Xtreme-CT",
  "experiment_id": "debug_ID000000",
  "run_name": "mDCSRN_test_",
  "note": "Enter note about experiment here",

  "is_train": true,

  "gpu_ids": [0],
  "dist": false,
  "up_factor": 1,  // upscaling factor

  "path": {
    "root": "superresolution",
    "pretrained_netG": null,
    "pretrained_netE": null,
    "pretrained_optimizerG": null
  },

  "datasets": {
      "name": "IXI",  // "TEST_DATASET" "2022_QIM_52_Bone" "IXI" "HCP_1200" "KIRBY21" "BRATS2023" "2022_QIM_52_Bone_Block",
      "dataset_type": "MonaiSmartCacheDataset",   // "MonaiDataset" "MonaiSmartCacheDataset" "MonaiIterableDataset" "MonaiCacheDataset"

      "n_channels": 1,  // number of input channels of data samples
      "patch_size": 64,  // Low-resolution patch size. High-resolution has size LR*UP_FACTOR
      "downsampling_method": "linear",  // used when degradation type is "resize"
      "degradation_type": "kspace_trunc",  // "resize"
      "factor_truncate": 3.0,  // Used wen degradation type is "kspace_trunc"

    "train": {
      "dataset_params": {  // Used for MonaiSmartCacheDataset
        "patches_per_batch": 2,  // number of patches to sample for each batch
        "replace_rate": 0.10,
        "cache_num": 20,
        "init_workers": 5,
        "replace_workers": 5
      },

      "dataloader_params": {  // Used for MonaiDataset
        "dataloader_shuffle": false,
        "dataloader_batch_size": 1,
        "num_load_workers": 0,
        "persist_workers": false,
        "pin_memory": false
      },

      "split": "train"
    },

    "test": {
      "dataset_params": {  // Used for MonaiSmartCacheDataset
        "patches_per_batch": 2,  // number of patches to sample for each batch
        "replace_rate": 0.10,
        "cache_num": 10,
        "init_workers": 5,
        "replace_workers": 5
      },

      "dataloader_params": {  // Used for MonaiDataset
        "dataloader_shuffle": true,
        "dataloader_batch_size": 1,
        "num_load_workers": 0,
        "persist_workers": false,
        "pin_memory": false
      },

      "split": "test"
    }
  },
  "netG": {
    "net_type": "mDCSRN",
    "description": "Multi-Level Densely Connected Network from mDCSRN-GAN paper",
    "in_channels": 1,
    "k_factor": 12,
    "k_size": 3,
    "num_dense_blocks": 4,
    "upsample_method": "deconv_nn_resize",
    "use_checkpoint": true,

    "init_type": "default",
    "init_bn_type": "uniform",      // "uniform" | "constant"
    "init_gain": 0.2
  }
  ,
  "train": {

    "epochs": 5,  // Maximum number of epochs
    "mixed_precision": "FP16",

    "G_loss_weights": {
      "MSE": 0,
      "L1": 1.0,
      "BCE_Logistic": 0,
      "BCE": 0,
      "VGG": 0,
      "VGG3D": 0.006,
      "GRAD": 0,
      "LAPLACE": 0,
      "TV3D": 0,  // 0.1
      "TEXTURE3D": 0,  // 0.5
      "ADV": 0,  // 10**-3,  // is 0.1 in mDCSRN-GAN paper, but uses WGAN-GP instead of vanilla GAN
      "STRUCTURE_TENSOR": 0  // 10**-7
    },

    "E_decay": 0,

    "G_optimizer_type": "adam",
    "G_optimizer_lr": 2e-4,
    "G_optimizer_wd": 0,

    "G_optimizer_clipgrad": null,
    "G_optimizer_reuse": true,
    "G_scheduler_type": "MultiStepLR",
    "G_scheduler_milestones": [
      250000,
      400000,
      450000,
      475000,
      500000
    ],

    "G_scheduler_gamma": 0.5,
    "G_regularizer_orthstep": null,
    "G_regularizer_clipstep": null,

    "G_param_strict": true,
    "E_param_strict": true,

    "manual_seed": 8323,
    "checkpoint_test": 20,
    "checkpoint_save": 1000000,
    "checkpoint_print": 20,
    "checkpoint_save_visuals": 20
  },
  "test": {
    "performance_metrics": ["psnr", "ssim", "nrmse"],
    "upscaling_methods": ["tio_nearest", "tio_linear"]
  }
}